<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/default_base_bg_disable"/>
        </shape>
    </item>
    <item
        android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/default_base_bg_press"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/default_base_white"/>
        </shape>
    </item>
</selector>