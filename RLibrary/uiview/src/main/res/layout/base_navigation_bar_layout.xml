<?xml version="1.0" encoding="utf-8"?>
<com.angcyo.uiview.github.tablayout.CommonTabLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/bottom_navigation_layout"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    android:clipChildren="false"
    android:clipToPadding="false"
    app:tl_indicator_height="0dp"
    app:tl_textSelectColor="@color/theme_color_accent"
    app:tl_textUnselectColor="@color/base_text_color_dark"
    app:tl_textsize="@dimen/default_text_size"
    app:tl_underline_color="@color/default_base_line"
    app:tl_underline_gravity="TOP"
    app:tl_underline_height="0dp"/>
