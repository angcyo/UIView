<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/base_big_round_radius"/>
            <solid android:color="@color/theme_color_primary_dark_tran"/>
            <stroke android:width="@dimen/base_border_size"
                    android:color="@color/default_base_border"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/base_big_round_radius"/>
            <stroke android:width="@dimen/base_border_size"
                    android:color="@color/default_base_border"/>
        </shape>
    </item>
</selector>
